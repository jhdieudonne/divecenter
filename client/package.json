    "@angular/router": "^5.1.0",
    "angular-bootstrap-md": "^5.1.2",
    "auth0-js": "^9.1.0",
    "chart.js": "^2.7.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.5.6"
  },
